func minValue(_ a : Int, _ b: Int) -> Int {
    if (a > b) {
        return b
    }
    else {
        return a
    }
}
 
print(minValue(5,2))
 
func lastDigit(a : Int) -> Int {
    return a%10
}
 
print(lastDigit(a:115))
 
func divides(_ a: Int, _ b: Int) -> Bool {
    if ( a % b == 0) {
        return true
    }
    else {
        return false
    }
}
 
print(divides(6,2))
 
func countDivisors(_ a: Int) -> Int {
    var counter = 0
    for i in 1...a {
        if(divides(a,i) == true) {
            counter += 1
        }
    }
    return counter
}
 
print(countDivisors(12))
 
func isPrime(_ a : Int) -> Bool {
    if(countDivisors(a) == 2) {
        return true
    }
    else {
        return false
    }
}
 
isPrime(3)
isPrime(8)
 
